{
  "name": "dowsing_kit",
  "version": "1.0.0",
  "main": "dist/index.js",
  "types" : "dist/index.d.ts",
  "author": "melnyczuk",
  "license": "MIT",
  "dependencies": {
    "@googlemaps/google-maps-services-js": "^2.0.2",
    "@types/node": "^13.7.2",
    "dotenv": "^8.2.0",
    "express": "^4.17.1",
    "ytdl-core": "^2.0.0",
    "ytsr": "^0.1.11"
  },
  "devDependencies": {
    "@types/express": "^4.17.2",
    "@types/jest": "^25.1.2",
    "@typescript-eslint/eslint-plugin": "^2.20.0",
    "@typescript-eslint/parser": "^2.20.0",
    "eslint": "^6.8.0",
    "eslint-config-prettier": "^6.10.0",
    "eslint-plugin-jest": "^23.7.0",
    "eslint-plugin-prettier": "^3.1.2",
    "husky": "^4.2.3",
    "jest": "^25.1.0",
    "lint-staged": "^10.0.7",
    "prettier": "^1.19.1",
    "ts-jest": "^25.2.0",
    "tslint": "^6.0.0",
    "tslint-config-prettier": "^1.18.0",
    "typescript": "^3.7.5"
  },
  "scripts": {
    "build": "tsc",
    "lint": "yarn lint:src && yarn lint:tst",
    "lint:fix": "yarn lint --fix",
    "lint:src": "eslint ./src/** --ext .ts",
    "lint:tst": "eslint ./tst/** --ext .ts",
    "package": "yarn build --esModuleInterop --outDir .dist src/**/*",
    "prettier": "prettier '{src,tst}/**/*.ts'",
    "prettier:fix": "yarn prettier --write",
    "start": "yarn build && node build/server/app.js",
    "start:dev": "yarn build --outDir .dummy dev/*.ts && cp -r dev/data .dummy && node .dummy/app.js",
    "test": "jest",
    "test:watch": "jest --watch",
    "typecheck": "tsc --noEmit"
  },
  "lint-staged": {
    "*.{js,ts}": [
      "yarn prettier:fix",
      "yarn lint:fix",
      "git add"
    ]
  },
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged"
    }
  },
  "description": "a toolkit for web discovery",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/melnyczuk/dowsing_kit.git"
  },
  "bugs": {
    "url": "https://github.com/melnyczuk/dowsing_kit/issues"
  },
  "homepage": "https://github.com/melnyczuk/dowsing_kit#readme"
}
